SELECT
'^' ||
array_to_string(
  knife_extract_text(
    '{"a": {"b": [{"c": "l", "d": "o"}, {"c": 1, "d": ["b", "o", "k"]}]}}',
    '[["a","b","c"],["a","b","d"]]'
  ),
  '$^'
)
|| '$'
;
